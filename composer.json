{
    "name": "symplify/monorepo-builder",
    "description": "Not only Composer tools to build a Monorepo.",
    "license": "MIT",
    "bin": [
        "bin/monorepo-builder"
    ],
    "require": {
        "magento/composer-dependency-version-audit-plugin": "~0.1",
        "magento/composer-root-update-plugin": "~2.0",
        "magento/product-enterprise-edition": "2.4.6-p4",
        "nette/utils": "^3.2",
        "phar-io/version": "^3.2",
        "php": ">=7.4.0",
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "6.1.*",
        "symfony/finder": "^6.2",
        "symfony/http-kernel": "^5.0",
        "symfony/process": "^6.2",
        "symplify/package-builder": "^11.1",
        "symplify/symplify-kernel": "^11.1"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "^2",
        "cweagans/composer-patches": "^1.7",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "dg/bypass-finals": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.8",
        "lusitanian/oauth": "^0.8",
        "magento/magento-coding-standard": "*",
        "magento/magento2-functional-testing-framework": "^4.3.1",
        "pdepend/pdepend": "^2.12",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.13",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.17",
        "sebastian/comparator": "<=4.0.6",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3.6",
        "symplify/easy-ci": "^11.3",
        "symplify/easy-coding-standard": "^12.0",
        "symplify/phpstan-extensions": "^11.1",
        "tomasvotruba/class-leak": "0.1.1.72",
        "tracy/tracy": "^2.9"
    },
    "autoload": {
        "exclude-from-classmap": [
            "apps/src/commerce-emea/**/Test/**",
            "apps/src/commerce-emea/**/dev/**",
            "apps/src/commerce-emea/**/update/**",
            "apps/src/commerce-rs/**/Test/**",
            "apps/src/commerce-rs/**/dev/**",
            "apps/src/commerce-rs/**/update/**"
        ],
        "files": [
            "apps/src/commerce-emea/app/etc/NonComposerComponentRegistration.php",
            "apps/src/commerce-rs/app/etc/NonComposerComponentRegistration.php"
        ],
        "psr-0": {
            "": [
                "apps/src/commerce-emea/app/code/",
                "apps/src/commerce-emea/generated/code/",
                "apps/src/commerce-rs/app/code/",
                "apps/src/commerce-rs/generated/code/"
            ]
        },
        "psr-4": {
            "Emea\\TestModuleTwo\\": "region/src/commerce-emea-bundle/Emea_TestModuleTwo/",
            "Emea\\TestModule\\": "region/src/commerce-emea-bundle/Emea_TestModule/",
            "Global\\MonoRepoPackage\\": "global/src/mono-repo-package/src",
            "Global\\SomePackage\\": "global/src/mono-repo-package/src",
            "Magento\\": [
                "apps/src/commerce-emea/app/code/Magento/",
                "apps/src/commerce-rs/app/code/Magento/"
            ],
            "Magento\\Framework\\": [
                "apps/src/commerce-emea/lib/internal/Magento/Framework/",
                "apps/src/commerce-rs/lib/internal/Magento/Framework/"
            ],
            "Magento\\Setup\\": [
                "apps/src/commerce-emea/setup/src/Magento/Setup/",
                "apps/src/commerce-rs/setup/src/Magento/Setup/"
            ],
            "Rs\\TestModuleTwo\\": "region/src/commerce-rs-bundle/Rs_TestModuleTwo/",
            "Rs\\TestModule\\": "region/src/commerce-rs-bundle/Rs_TestModule/",
            "Symplify\\MonorepoBuilder\\": [
                "packages",
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\PhpStan\\": [
                "apps/src/commerce-emea/dev/tests/static/framework/Magento/PhpStan/",
                "apps/src/commerce-rs/dev/tests/static/framework/Magento/PhpStan/"
            ],
            "Magento\\Sniffs\\": [
                "apps/src/commerce-emea/dev/tests/static/framework/Magento/Sniffs/",
                "apps/src/commerce-rs/dev/tests/static/framework/Magento/Sniffs/"
            ],
            "Magento\\TestFramework\\Inspection\\": [
                "apps/src/commerce-emea/dev/tests/static/framework/Magento/TestFramework/Inspection/",
                "apps/src/commerce-rs/dev/tests/static/framework/Magento/TestFramework/Inspection/"
            ],
            "Magento\\TestFramework\\Utility\\": [
                "apps/src/commerce-emea/dev/tests/static/framework/Magento/TestFramework/Utility/",
                "apps/src/commerce-rs/dev/tests/static/framework/Magento/TestFramework/Utility/"
            ],
            "Magento\\Tools\\": [
                "apps/src/commerce-emea/dev/tools/Magento/Tools/",
                "apps/src/commerce-rs/dev/tools/Magento/Tools/"
            ],
            "Magento\\Tools\\Sanity\\": [
                "apps/src/commerce-emea/dev/build/publication/sanity/Magento/Tools/Sanity/",
                "apps/src/commerce-rs/dev/build/publication/sanity/Magento/Tools/Sanity/"
            ],
            "Symplify\\MonorepoBuilder\\Tests\\": [
                "packages-tests",
                "tests"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "extra": {
        "enable-patching": true,
        "magento-force": "override",
        "patches": {
            "symfony/dependency-injection": [
                "https://raw.githubusercontent.com/symplify/vendor-patch-files/main/patches/generic-php-config-loader.patch"
            ]
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "vendor/bin/phpstan analyse --ansi  --error-format symplify",
        "rector": "vendor/bin/rector process --dry-run --ansi",
        "release": "bin/monorepo-builder release patch --ansi"
    },
    "replace": {
        "commerce-emea-bundle/module-test-module": "self.version",
        "commerce-emea-bundle/module-test-module-two": "self.version",
        "commerce-rs-bundle/module-test-module": "self.version",
        "commerce-rs-bundle/module-test-module-two": "self.version",
        "global/module-mono-repo-package": "self.version",
        "magento/project-enterprise-edition": "self.version"
    },
    "minimum-stability": "a",
    "prefer-stable": true
}
