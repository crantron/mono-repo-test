<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\AdobeCommerceOutOfProcessExtensibility\Model\Generator\ClassGeneratorInterface" type="Magento\AdobeCommerceEventsGenerator\Generator\EventingClassGenerator"/>
    <type name="Magento\Framework\Console\CommandListInterface">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="eventModuleGenerateCommand" xsi:type="object">
                    Magento\AdobeCommerceEventsGenerator\Console\Command\GenerateModuleCommand
                </item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\AdobeCommerceEventsGenerator\Generator\EventingClassGenerator">
        <arguments>
            <argument name="templateEngine" xsi:type="object">Magento\Framework\View\TemplateEngine\Php</argument>
        </arguments>
    </type>
    <type name="Magento\AdobeCommerceEventsGenerator\Console\Command\GenerateModule\Generator">
        <arguments>
            <argument name="apiServiceCollector" xsi:type="object">Magento\AdobeCommerceOutOfProcessExtensibility\Model\Generator\Collector\ApiServiceCollector\Proxy</argument>
            <argument name="moduleCollector" xsi:type="object">Magento\AdobeCommerceOutOfProcessExtensibility\Model\Generator\Collector\ModuleCollector\Proxy</argument>
            <argument name="resourceModelCollector" xsi:type="object">Magento\AdobeCommerceOutOfProcessExtensibility\Model\Generator\Collector\ResourceModelCollector\Proxy</argument>
        </arguments>
    </type>
</config>
